{
  "name": "nextjs-checkout",
  "version": "1.0.0",
  "description": "A superfast and full featured shopping cart built with Next.js and Prisma ORM.",
  "scripts": {
    "dev": "next",
    "turbo": "turbo build",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db": "npx prisma db push",
    "db:local": "npx dotenv -e .env.local prisma db push",
    "db:test": "npx dotenv -e .env.test prisma db push",
    "seed": "npx ts-node prisma/seed.mjs",
    "seed:test": "npx dotenv -e .env.test ts-node prisma/seed.mjs",
    "test": "npx dotenv -e .env.test -- jest --verbose ./__tests__/specs --runInBand",
    "test:ui": "npx playwright test"
  },
  "prisma": {
    "seed": "npx ts-node prisma/seed.mjs"
  },
  "keywords": [
    "cart",
    "shopping",
    "website",
    "stripe",
    "square",
    "verifone",
    "nextjs",
    "react",
    "typescript",
    "shopping cart"
  ],
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.354.0",
    "@prisma/client": "^5.7.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "array-move": "^4.0.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "date-fns": "^2.30.0",
    "formidable": "^3.4.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "micro-cors": "^0.1.1",
    "mime-types": "^2.1.35",
    "next": "^13.2.4",
    "next-auth": "^4.22.1",
    "object-hash": "^3.0.0",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-bootstrap-icons": "^1.10.2",
    "react-chartjs-2": "^5.2.0",
    "react-cookie": "^4.1.1",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-easy-sort": "^1.5.1",
    "react-number-format": "^5.2.2",
    "react-quill": "^2.0.0",
    "react-responsive-carousel": "^3.2.23",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.2",
    "redux": "4.2.0",
    "sharp": "^0.32.0",
    "square": "^33.1.0",
    "stripe": "^11.17.0",
    "swr": "^2.0.0",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "@types/jest": "^29.5.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^18.16.2",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
